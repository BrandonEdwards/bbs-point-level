	pages = {87--98},
	file = {Sauer and Link - 2011 - Analysis of the North American Breeding Bird Surve.pdf:/home/brandon/Zotero/storage/MAMGPP8H/Sauer and Link - 2011 - Analysis of the North American Breeding Bird Surve.pdf:application/pdf},
}

@article{matsuoka_using_2012,
